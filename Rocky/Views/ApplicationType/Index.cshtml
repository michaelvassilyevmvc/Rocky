@model IEnumerable<Rocky.Models.ApplicationType>


<div class="container p-3">
  <div class="row pt-4">
    <div class="col-6">
      <h2 class="text-primary">ApplicationType List</h2>
    </div>
    <div class="col-6 text-right">
      <a asp-action="Create" asp-controller="ApplicationType" asp-area="" class="btn btn-primary">Create New ApplicationType</a>
      
    </div>
  </div>
</div>
<br /><br />
@if(Model.Count() > 0)
{
  <table class="table table-bordered table-striped" style="width:100%">
    <thead>
      <tr>
        <th>ApplicationType Name</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var item in Model)
      {
        <tr>
          <td width="50%">@item.Name</td>
          <td></td>
        </tr>
      }
    </tbody>
  </table>
}
else{
  <p>No ApplicationType exists.</p>
}
  
